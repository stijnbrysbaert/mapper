@prefix map: <http://mapping.example.com/>.
@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.

map:rules_000 a <http://rdfs.org/ns/void#Dataset>;
    <http://rdfs.org/ns/void#exampleResource> map:map_person_000.
map:map_person_000 rml:logicalSource map:source_000.
map:source_000 a rml:LogicalSource;
    rml:source "../data/data.json";
    rml:iterator "$.persons[*]";
    rml:referenceFormulation ql:JSONPath.
map:map_person_000 a rr:TriplesMap;
    rdfs:label "person".
map:s_000 a rr:SubjectMap.
map:map_person_000 rr:subjectMap map:s_000.
map:s_000 rr:template "http://example.com/{firstname}".
map:pom_000 a rr:PredicateObjectMap.
map:map_person_000 rr:predicateObjectMap map:pom_000.
map:pm_000 a rr:PredicateMap.
map:pom_000 rr:predicateMap map:pm_000.
map:pm_000 rr:constant rdf:type.
map:pom_000 rr:objectMap map:om_000.
map:om_000 a rr:ObjectMap;
    rr:constant "http://xmlns.com/foaf/0.1/Person";
    rr:termType rr:IRI.
map:pom_001 a rr:PredicateObjectMap.
map:map_person_000 rr:predicateObjectMap map:pom_001.
map:pm_001 a rr:PredicateMap.
map:pom_001 rr:predicateMap map:pm_001.
map:pm_001 rr:constant <http://example.com/name>.
map:pom_001 rr:objectMap map:om_001.
map:om_001 a rr:ObjectMap;
    rml:reference "firstname";
    rr:termType rr:Literal.
